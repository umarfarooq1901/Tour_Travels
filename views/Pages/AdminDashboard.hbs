<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Admin Dashboard</title>
  

</head>
<body>
   

    <!-- Main Content -->
    <div class="container">
     
        <h1 class="text-center my-5">Welcome, {{adminName}}!</h1>

        <div>
            <h3 class="my-5 text-center bg-dark text-white rounded p-3">Registered Users</h3>

            <table class="table table-hover">
                <thead>
                    <tr>
                        <th scope="col">Name</th>
                        <th scope="col">Email</th>
                        <th scope="col">Contact</th>
                        <th scope="col">Action</th>
                     
                    </tr>
                </thead>
                <tbody>
                    {{#each users}}
                        <tr>
                            <td>{{this.username}}</td>
                            <td>{{this.email}}</td>
                            <td>{{this.contact}}</td>
                            <td><button>Edit</button><button>Delete</button> </td>
                        </tr>
                    {{/each}}
                </tbody>
            </table>
        </div>
    </div>

    

</body>
</html>
